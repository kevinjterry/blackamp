  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#d3dade"
    },
    "variable.constant": {
      "foreground": "#d3dade"
    },
    "variable.defaultLibrary": {
      "foreground": "#458b96"
    },
    "type": "#458b96",
    "class": "#458b96",
    "type.constructor": "#c8a368",
    "class.constructor": "#c8a368",
    "function.constructor": "#c8a368",
    "method.constructor": "#c8a368",
    "function.call": "#e0e3e6",
    "method.call": "#e0e3e6"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#585958"
      }
    },
    {
      "name": "Comment Markup Link",
      "scope": "comment markup.link",
      "settings": {
        "foreground": "#585958"
      }
    },
    {
      "name": "Comment storage type",
      "scope": ["comment storage.type", "comment keyword"],
      "settings": { "foreground": "#9ba1a3" }
    },

    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Constant other",
      "scope": ["constant.other"],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Variable constants",
      "scope": ["variable.other.constant"],
      "settings": { "foreground": "#458b96" }
    },

    {
      "name": "Entity name functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method",
        "variable.function"
      ],
      "settings": { "foreground": "#e0e3e6" }
    },
    {
      "name": "Function calls",
      "scope": [
        "meta.function-call.generic.python",
        "support.function.builtin.python"
      ],
      "settings": { "foreground": "#e0e3e6" }
    },
    {
      "name": "Support functions",
      "scope": ["support.function"],
      "settings": { "foreground": "#d3dade" }
    },

    {
      "name": "Classes",
      "scope": [
        "support.class",
        "entity.name.type.class",
        "entity.name.class",
        "variable.other.class.js",
        "variable.other.class.ts"
      ],
      "settings": { "foreground": "#458b96" }
    },
    {
      "name": "Type names",
      "scope": ["entity.name.type", "entity.name.type.namespace"],
      "settings": { "foreground": "#458b96" }
    },
    {
      "name": "Constructor calls",
      "scope": [
        "meta.function-call entity.name.type",
        "meta.instance.constructor entity.name.type",
        "variable.function entity.name.type",
        "meta.function-call.constructor entity.name.type",
        "entity.name.function.constructor"
      ],
      "settings": { "foreground": "#c8a368" }
    },

    {
      "name": "Keywords",
      "scope": ["keyword"],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "Keyword control",
      "scope": ["keyword.control"],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "JS/TS Keywords",
      "scope": [
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof"
      ],
      "settings": { "foreground": "#c8a368" }
    },

    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "Arithmetic operators",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Assignment operators",
      "scope": ["keyword.operator.assignment"],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Compound assignment operators",
      "scope": ["keyword.operator.assignment.compound"],
      "settings": { "foreground": "#c8a368" }
    },

    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "Storage type",
      "scope": ["storage.type"],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "Storage modifiers",
      "scope": ["storage.modifier", "storage.type.modifier"],
      "settings": { "foreground": "#c8a368" }
    },

    {
      "name": "Strings",
      "scope": ["string"],
      "settings": { "foreground": "#959695" }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "Template literals",
      "scope": ["keyword.other.template.begin", "keyword.other.template.end"],
      "settings": { "foreground": "#959695" }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": { "foreground": "#d4d4d4" }
    },

    {
      "name": "Variables",
      "scope": ["variable"],
      "settings": { "foreground": "#d36464" }
    },
    {
      "name": "Variable parameters",
      "scope": ["variable.parameter.function"],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": { "foreground": "#c8a368" }
    },
    {
      "name": "Variable properties",
      "scope": [
        "variable.other.readwrite",
        "meta.object-literal.key",
        "support.variable.property"
      ],
      "settings": { "foreground": "#d36464" }
    },

    {
      "name": "Punctuation",
      "scope": ["punctuation.separator.delimiter"],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "Punctuation definition",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end"
      ],
      "settings": { "foreground": "#959695" }
    },

    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#d36464" }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Attribute IDs",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#e0e3e6"
      }
    },

    {
      "name": "Markup headings",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#d36464" }
    },
    {
      "name": "Markup bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d3dade"
      }
    },
    {
      "name": "Markup italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c8a368"
      }
    },

    {
      "name": "JSON Property Name",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#d36464" }
    },
    {
      "name": "JSON String",
      "scope": [
        "source.json meta.structure.dictionary.json > value.json > string.quoted.json"
      ],
      "settings": { "foreground": "#959695" }
    },

    {
      "name": "Python self parameter",
      "scope": ["variable.parameter.function.language.special.self.python"],
      "settings": { "foreground": "#458b96" }
    },
    {
      "name": "Python function parameters",
      "scope": ["variable.parameter.function.language.python"],
      "settings": { "foreground": "#d3dade" }
    },
    {
      "name": "Python logical operators",
      "scope": ["keyword.operator.logical.python"],
      "settings": { "foreground": "#c8a368" }
    },

    {
      "name": "TypeScript/JavaScript primitives",
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": { "foreground": "#458b96" }
    },
    {
      "name": "JS/TS function parameter",
      "scope": ["variable.parameter.function.js"],
      "settings": { "foreground": "#d36464" }
    },

    {
      "name": "Rust storage modifiers",
      "scope": ["storage.modifier.lifetime.rust"],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "Rust entity names",
      "scope": ["entity.name.lifetime.rust"],
      "settings": { "foreground": "#458b96" }
    },
    {
      "name": "Rust language variables",
      "scope": ["variable.language.rust"],
      "settings": { "foreground": "#d36464" }
    },

    {
      "name": "Invalid/Error",
      "scope": ["invalid.illegal"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": { "foreground": "#ffffff" }
    },

    {
      "name": "Italic emphasis",
      "scope": [
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.ts",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "variable.parameter",
        "variable.language.super",
        "comment.line.double-slash",
        "comment.block.documentation",
        "keyword.control.import.python",
        "keyword.control.flow.python",
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
